{
  "compilerOptions": {
    "outDir": "./.build",
    "target": "es5",
    "sourceMap": true,
    "lib": ["es6", "dom"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": false,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "jsx": "react"
  },
  "exclude": ["node_modules", "config", "dev_server.js"]
}
